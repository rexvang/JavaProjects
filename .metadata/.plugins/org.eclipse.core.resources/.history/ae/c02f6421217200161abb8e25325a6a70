
public class Player {
	private String name;
	private int speed;
	private int load;
	private int health;
	private 
	
	public Player(String name, int speed, int load, int health) {
		this.name = name;
		this.speed = speed;
		this.load = load;
		this.health = health;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public int getSpeed() {
		return speed;
	}
	public void setSpeed(int speed) {
		this.speed = speed;
	}
	public int getLoad() {
		return load;
	}
	public void setLoad(int load) {
		this.load = load;
	}
	public int getHealth() {
		return health;
	}
	public void setHealth(int health) {
		this.health = health;
	}
	
	public void carry(String itemName, int quantity, int weight) {
		
	}
	
	public void drop(String itemName, int quantity) {
		
	}
}
